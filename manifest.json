{
	"name": "Github Gists",
	"description": "Simple view for github gists",
	"manifest_version": 2,
	"version": "1.0",

	"icons": {
		"24": "assets/24.png",
		"32": "assets/32.png",
		"120": "assets/120.png"
	},

	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"storage"
	]
}
